{%
  set classes = [
    'field',
    'field--name-' ~ field_name|clean_class,
    'field--type-' ~ field_type|clean_class,
    'field--label-' ~ label_display,
  ]
%}
{%
  set title_classes = [
    'field--label',
    label_display == 'visually_hidden' ? 'sr-only',
  ]
%}
<div class="slider-rating">
  {% for item in items %}
    <div class="slider-rating__item">
      {{ item.content }}
    </div>
  {% endfor %}
</div>
